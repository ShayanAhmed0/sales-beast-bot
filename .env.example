# AI Voice Sales Agent Configuration
# Copy this file to .env and fill in your actual API keys

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# SendGrid Configuration (for email follow-ups)
SENDGRID_API_KEY=your_sendgrid_api_key_here
SENDGRID_FROM_EMAIL=your_from_email_here

# Database Configuration
DATABASE_URL=sqlite:///app.db

# Flask Configuration
FLASK_ENV=development
SECRET_KEY=your_secret_key_here

# Voice Agent Configuration
DEFAULT_VOICE_ID=your_default_elevenlabs_voice_id
VOICE_STABILITY=0.5
VOICE_SIMILARITY_BOOST=0.8

# Call Configuration
MAX_CALL_DURATION=600  # 10 minutes in seconds
CALL_TIMEOUT=30  # seconds to wait for answer
RETRY_ATTEMPTS=3  # number of retry attempts for failed calls

# Analytics Configuration
ANALYTICS_RETENTION_DAYS=90  # days to keep detailed analytics data

